{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 120,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-sort-re-exports", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^@angular/(.*)$", "^rxjs/(.*)$", "<THIRD_PARTY_MODULES>", "^@gov/(.*)$", "^@core/(.*)$", "^@shared/(.*)$", "^[./]"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
